df %>% group_by(class, family, genus, species) %>% 
  summarize(n())


df %>% mutate(total=n()) %>% 
  filter(family != 'U') %>% 
  mutate(t.family=n(), p.family=t.family/total*100) %>% 
  filter(genus != 'U') %>% 
  mutate(t.genus=n(), p.genus=t.genus/total*100) %>% 
  dplyr::select(p.family, p.genus) %>% 
  distinct()
