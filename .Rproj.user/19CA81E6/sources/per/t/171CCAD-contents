ggplot(ml.tz, aes(x=amt.tz, y=ratio, label=site)) +
  geom_point() +
  geom_text(hjust='inward', nudge_y = 0.5) +
  theme_classic()

ggplot(ml.tz, aes(x=amt.tz, y=log(ratio), label=site)) +
  geom_point() +
  geom_text(hjust='inward', nudge_y = 0.5) +
  theme_classic()

pr.ml.tz <- predict(ratio.x.tz)

plot(pr.ml.tz)

residuals(ratio.x.tz) %>% plot()

