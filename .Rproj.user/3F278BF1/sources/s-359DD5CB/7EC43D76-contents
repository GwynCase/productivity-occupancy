diet.items %>% filter(genus == 'Larus')

# Create a function to calculate n.
give.n <- function(x){
  return(c(y = mean(x), label = length(x)))
}

ggplot(diamonds, aes(cut, price)) + 
  geom_boxplot() + 
  stat_summary(fun.data = give.n, geom = "text")

library(QuantPsyc)

test <- glm(cbind(years.occupied, years.empty) ~ proportion.suitable, data=data, 
    family=binomial(logit), weights=years.surveyed)

test2 <- glm(cbind(years.occupied, years.empty) ~ bec.diversity, data=data, 
             family=binomial(logit), weights=years.surveyed)

test3 <- glm(cbind(years.occupied, years.empty) ~ bec.diversity + proportion.suitable, data=data, 
             family=binomial(logit), weights=years.surveyed)

ggcoef(test, exclude_intercept=TRUE) +
  theme_classic()
