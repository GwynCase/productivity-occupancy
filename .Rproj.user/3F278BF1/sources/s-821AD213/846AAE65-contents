diet.items %>% filter(genus == 'Larus')

# Create a function to calculate n.
give.n <- function(x){
  return(c(y = mean(x), label = length(x)))
}

ggplot(diamonds, aes(cut, price)) + 
  geom_boxplot() + 
  stat_summary(fun.data = give.n, geom = "text")


# Here's how to get the link from one model:
family(top.model)$linkinv

family(top.model) %>% str()

# Here's models.
pfa.models

# Get family from all models.
map(pfa.models$model, family)
pfa.models %>% mutate(family=map(model, family))
fams <- pfa.models %>% mutate(family=map(model, family))

# How to select sub items?
fams$family[[9]]$linkinv















